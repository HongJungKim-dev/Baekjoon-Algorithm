#include <iostream>

using namespace std;

void make_battleground(char battleground[][101], int row, int col);

int main() {
	int row = 0;
	int col = 0;
	int size = 0;
	int i = 0, j = 0;
	char battleground[101][101];

	cin >> row;
	cin >> col;
	make_battleground(battleground, row, col);
}

void make_battleground(char battleground[][101], int row, int col) {
	int i = 0;
	int j = 0;
	char soldier = 0;

	for (i = 0; i < row; i++) {
		for (j = 0; j < col; j++) {
			cin >> battleground[i][j];
		}
	}

}
